{
  "name": "rpitv-website",
  "version": "1.0.0",
  "description": "RPI TV Website",
  "author": "Erik Roberts",
  "private": true,
  "scripts": {
    "dev": "dotenv -- nuxt",
    "build": "dotenv -- nuxt build",
    "start": "dotenv -- nuxt start",
    "generate": "dotenv -- nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "cy:open": "cypress open",
    "test": "npm run test:jest && npm run test:cy",
    "test:jest": "jest",
    "pretest:cy": "rm -rf .nyc_output || true",
    "test:cy": "dotenv -- cypress run --record",
    "posttest": "npm run report:combined",
    "copy:reports": "cp coverage/cypress/coverage-final.json coverage/from-cypress.json && cp coverage/jest/coverage-final.json coverage/from-jest.json",
    "precombine:reports": "npm run copy:reports && mkdir .nyc_output || true && node combine-reports-data-flattening.js",
    "combine:reports": "npx nyc merge coverage && mv coverage.json .nyc_output/out.json",
    "prereport:combined": "npm run combine:reports",
    "report:combined": "npx nyc report --reporter text --report-dir coverage"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-brands-svg-icons": "^5.12.0",
    "@fortawesome/pro-duotone-svg-icons": "^5.12.0",
    "@fortawesome/pro-light-svg-icons": "^5.12.0",
    "@fortawesome/vue-fontawesome": "^0.1.9",
    "@nuxtjs/apollo": "^4.0.0-rc19",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/dotenv": "^1.4.0",
    "@nuxtjs/pwa": "^3.0.0-beta.19",
    "@nuxtjs/sentry": "^4.0.3",
    "@types/jest": "^24.0.23",
    "axios": "^0.21.1",
    "graphql-tag": "^2.10.3",
    "http-proxy-middleware": "^1.0.3",
    "moment": "^2.24.0",
    "nuxt": "^2.0.0",
    "regenerator-runtime": "^0.13.3",
    "simplebar-vue": "^1.4.0",
    "vue-the-mask": "^0.11.1",
    "vuetify": "^2.1.14"
  },
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@cypress/code-coverage": "^3.7.2",
    "@nuxt/typescript-build": "^0.5.1",
    "@nuxtjs/eslint-config": "^1.0.1",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/vuetify": "^1.9.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "cypress": "^4.5.0",
    "dotenv-cli": "^3.1.0",
    "eslint": "^6.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-jest-dom": "^1.4.2",
    "eslint-plugin-nuxt": ">=0.4.2",
    "fs-extra": "^9.0.0",
    "identity-obj-proxy": "^3.0.0",
    "istanbul-lib-coverage": "^3.0.0",
    "jest": "^25.5.4",
    "jest-serializer-vue": "^2.0.2",
    "node-sass": "^4.14.1",
    "nyc": "^15.0.1",
    "sass-loader": "^8.0.0",
    "sinon": "^7.5.0",
    "ts-jest": "^24.2.0",
    "vue": "latest",
    "vue-jest": "^3.0.5",
    "vue-router": "^3.2.0",
    "vuex": "^3.1.3"
  },
  "nyc": {
    "extension": [
      ".vue"
    ],
    "report-dir": "coverage/cypress",
    "exclude": [
      ".nuxt/**/*"
    ],
    "include": [
      "components/**/*",
      "layouts/**/*",
      "middleware/**/*",
      "pages/**/*",
      "plugins/**/*",
      "store/**/*"
    ]
  }
}
